function bubble_sort(a)
  var changed = 1;
  var count = getn(a);
  var temp;

  while (changed == 1) do
    changed = 0;
    count = count - 1;
    for i in 1..count do
      if (a[i] > a[i + 1]) do
        temp = a[i];
        a[i] = a[i+1];
        a[i+1] = temp;
        changed = 1;
      end
    end
  end

  return a;
end

each item in bubble_sort([2,9,4,5,1,6,3]) do
  print(item);
end
